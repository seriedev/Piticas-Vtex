<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:vtex="http://www.vtex.com.br/2009/vtex-common"
  xmlns:vtex.cmc="http://www.vtex.com.br/2009/vtex-commerce" lang="pt-br">

<head>
  <%if (process.env.NODE_ENV === 'development') { %>
  <%- require('ejs-webpack-loader!./partials/subtemplates/head.ejs')() -%>
  <% } else {%>
  <vtex:template id="FRN-Head" />
  <link rel="stylesheet" href="/arquivos/frn.lojapiticas.category.css" />
  <% }  %>
</head>

<body>
  <%if (process.env.NODE_ENV === 'development') { %>
  <%- require('ejs-webpack-loader!./partials/subtemplates/header.ejs')() -%>
  <% } else {%>
  <vtex:template id="FRN-Header" />
  <% }  %>

  <main>
    <!--<%if (process.env.NODE_ENV === 'development') { %>
      <%- require('ejs-webpack-loader!./partials/subtemplates/storeBenefits.ejs')() %>
    <% } else {%>
      <vtex:template id="frn-storeBenefits" />
    <% }  %>-->

    <section class="banner-category">
      <div class="banner-category__content">
        <%if (process.env.NODE_ENV === 'development') { %>
        <h2>categoria</h2>
        <picture class="box-banner">
          <source media="(max-width: 480px)" srcset="/arquivos/banner-category-mobile.png"><img
            src="/arquivos/banner-category.png" alt="" srcset="" />
        </picture>
        <% } else {%>
        <vtex:contentPlaceHolder id="Category Banner" />
        <% }  %>
      </div>
    </section>

    <!--<%if (process.env.NODE_ENV === 'development') { %>
      <%- require('ejs-webpack-loader!./partials/subtemplates/brands.ejs')() %>
    <% } else {%>
      <vtex:template id="frn-brands" />
    <% }  %>-->


    <div class="category__content"> 
      <div class="filter-overlay">
        <div class="icon-close-filter-right">
          <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 32 32">
            <path
              d="M10.05 23.95c.39.39 1.024.39 1.414 0L17 18.414l5.536 5.536c.39.39 1.024.39 1.414 0 .39-.39.39-1.024 0-1.414L18.414 17l5.536-5.536c.39-.39.39-1.024 0-1.414-.39-.39-1.024-.39-1.414 0L17 15.586l-5.536-5.536c-.39-.39-1.024-.39-1.414 0-.39.39-.39 1.024 0 1.414L15.586 17l-5.536 5.536c-.39.39-.39 1.024 0 1.414z" />
          </svg>
        </div>        
      </div>     
      <section class="filter js-filter">              
        <div class="filter__title-desktop">
          Filtrar
          <a class="filter__reset-button" onClick="window.location.reload();">Limpar Filtros</a>
        </div>        
        <!--<div class="filter__title-mobile"><a class="filter__header js-filter-mobile" href="javascript:;">Filtrar
            <svg class="filter__icon filter__icon--close" width="16" height="16" viewBox="0 0 16 16" fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" clip-rule="evenodd"
                d="M8 6.54545L1.45455 0L0 1.45455L6.54545 8L0 14.5455L1.45455 16L8 9.45455L14.5455 16L16 14.5455L9.45455 8L16 1.45455L14.5455 0L8 6.54545Z"
                fill="#014080"></path>
            </svg></a>
        </div>-->
        <div class="filter__wrapper">
          <div class="filter__especification">
            <ul class="filter__list js-filter-list"></ul>
          </div>
          <div class="filter-selected">
            <div class="filter-selected__wrapper">
              <div class="filter-selected__title js-order-by-current"><span>Você filtrou por:</span></div>
              <div class="filter__selected-items"></div>
            </div>
          </div>
        </div>        
      </section>
      <div class="order-overlay">
        <div class="icon-close-filter-left">
          <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 32 32">
            <path
              d="M10.05 23.95c.39.39 1.024.39 1.414 0L17 18.414l5.536 5.536c.39.39 1.024.39 1.414 0 .39-.39.39-1.024 0-1.414L18.414 17l5.536-5.536c.39-.39.39-1.024 0-1.414-.39-.39-1.024-.39-1.414 0L17 15.586l-5.536-5.536c-.39-.39-1.024-.39-1.414 0-.39.39-.39 1.024 0 1.414L15.586 17l-5.536 5.536c-.39.39-.39 1.024 0 1.414z" />
          </svg>
        </div>        
      </div>
      <div class="filter__orderBy--mobile js-filter-order">
        <div class="filter__title-desktop js-order-by-current">Ordernar por</div>
        <div class="orderBy">
          <ul class="orderBy__list--mobile js-filter-orderBy-list">
            <li class="orderBy__item--mobile"><a class="orderBy__action--mobile js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByReleaseDateDESC">Lançamento</a></li>
            <li class="orderBy__item--mobile"><a class="orderBy__action--mobile js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByNameASC">Nome</a></li>
            <li class="orderBy__item--mobile"><a class="orderBy__action--mobile js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByPriceASC">Preço</a></li>
            <li class="orderBy__item--mobile"><a class="orderBy__action--mobile js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByTopSaleDESC">Mais vendidos</a></li>
            <!-- <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByPriceDESC">Maior Preço</a></li>
            <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByReviewRateDESC">Melhores avaliações</a></li>
            <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByNameDESC">Z - A</a></li>
            <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                data-filter-value="OrderByBestDiscountDESC">Melhor Desconto</a></li> -->
          </ul>
        </div>
      </div>
      <div class="category__content--last">
        <div class="breadCrumb__container">
          <%if (process.env.NODE_ENV === 'development') { %>
          <%- require('ejs-webpack-loader!./partials/components/breadcrumbs.html')() %>
          <% } else {%>
          <vtex.cmc:breadcrumb />
          <% }  %>
          <div class="filter__orderBy">
            <div class="orderBy">
              <ul class="orderBy__list js-filter-orderBy-list">
                <li class="orderBy__title">Ordernar por:</li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByReleaseDateDESC">Lançamento</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByNameASC">Nome</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByPriceASC">Preço</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByTopSaleDESC">Mais vendidos</a></li>
                <!-- <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByPriceDESC">Maior Preço</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByReviewRateDESC">Melhores avaliações</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByNameDESC">Z - A</a></li>
                <li class="orderBy__item"><a class="orderBy__action js-filter-orderBy-item" href="javascript:;"
                    data-filter-value="OrderByBestDiscountDESC">Melhor Desconto</a></li> -->
              </ul>
            </div>
          </div>
        </div>
        <section class="filter-mobile">
          <div class="filter-mobile__wrapper">
            <div class="filter-mobile__especification filter-mobile__open">
              <a class="filter-mobile__link" href="#">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="16" viewBox="0 0 32 32">
                  <path d="M27 6h-3c0-1.104-.896-2-2-2h-2c-1.104 0-2 .896-2 2H5c-.552 0-1 .448-1 1s.448 1 1 1h13c0 1.104.896 2 2 2h2c1.104 0 2-.896 2-2h3c.552 0 1-.448 1-1s-.448-1-1-1zm-5 2h-2V6h2v2zm5 6H14c0-1.104-.896-2-2-2h-2c-1.104 0-2 .896-2 2H5c-.552 0-1 .448-1 1s.448 1 1 1h3c0 1.104.896 2 2 2h2c1.104 0 2-.896 2-2h13c.552 0 1-.448 1-1s-.448-1-1-1zm-15 2h-2v-2h2v2zm15 6h-7c0-1.104-.896-2-2-2h-2c-1.104 0-2 .896-2 2H5c-.552 0-1 .448-1 1s.448 1 1 1h9c0 1.104.896 2 2 2h2c1.104 0 2-.896 2-2h7c.552 0 1-.448 1-1s-.448-1-1-1zm-9 2h-2v-2h2v2z"/>
                </svg>
                Filtrar</a>
            </div>
            <div class="filter-mobile__orderBy orderBy-mobile__open">
              <a class="filter-mobile__link" href="#">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="18" viewBox="0 0 32 32.002">
                  <path d="M18.28 31.718c.196.196.454.294.71.292.258 0 .514-.096.71-.292l3.852-3.852c.39-.39.39-1.024 0-1.414s-1.024-.39-1.414 0L20 28.59V11.01c0-.552-.448-1-1-1s-1 .448-1 1v17.6l-2.304-2.304c-.39-.39-1.024-.39-1.414 0-.39.39-.39 1.024 0 1.414l3.998 3.998zM13 22.01c.552 0 1-.448 1-1V3.41l2.304 2.304c.39.39 1.024.39 1.414 0 .39-.39.39-1.024 0-1.414L13.72.302c-.196-.196-.452-.292-.71-.292-.258 0-.514.096-.71.292L8.448 4.154c-.39.39-.39 1.024 0 1.414.39.39 1.024.39 1.414 0L12 3.43v17.58c0 .552.448 1 1 1z"/>
                </svg>
                Ordernar</a>
            </div>
          </div>
        </section>
        <section class="vitrine">        
          <div class="vitrine__wrapper">
            <div class="vitrine__results">
              <ul class="vitrine__group"></ul>
            </div>
          </div>
          <div class="vitrine__loadmore"> </div>
        </section>        
        <div class="filtro">
          <h2 class="filter__title">Filtros
            <div class="content">
              <div id="app"></div>
            </div>
            <button class="js-reset-filter">Limpiar Filtro</button>
            <button class="js-apply-filter">Aplicar Filtro</button>
          </h2>
        </div>
      </div>     
    </div>    
  </main>

  <div class="hide">
    <%if (process.env.NODE_ENV !== 'development') { %>
      <vtex.cmc:searchResult layout="2581f661-fc47-4829-9361-778c3f033794" itemCount="1" columnCount="1" />
    <% }%>
  </div>

  <%if (process.env.NODE_ENV === 'development') { %>
  <%- require('ejs-webpack-loader!./partials/subtemplates/footer.ejs')() -%>
  <%- require('ejs-webpack-loader!./partials/subtemplates/scripts.ejs')() -%>
  <% } else {%>
  <vtex:template id="FRN-Footer" />
  <vtex:template id="FRN-Scripts" />
  <script src="//code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
  <script src="/arquivos/frn.lojapiticas.category.js"></script>
  <% }  %>
</body>

</html>